<%= @current_user.name %> <%= @current_user.email %>
<div class="inner ">
  <div class="row">
    <% @articles.each do |article| %>
    <div class="col s12 m12">
      <div class="card blue darken-1">
        <div class="card-content white-text">
          <span class="card-title"></span>
          <p>"<%= link_to  article.title, user_article_path(id: article.id) %>"</p>
          <p><%= article.published_at%></p>
          <p>URL:<%= article.url%></p>
          <p><%= link_to  "Delete", user_article_path(id: article.id), method: :delete %></p>
        </div>
        <div class="card-action">
        </div>
      </div>
    </div>
    <% end %>
  </div>
    <p><%= link_to "記事の新規投稿" , new_user_article_path(current_user) %></p>
</div>
