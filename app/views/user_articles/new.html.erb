<%= form_with model:@article, url:user_articles_path, method: :post, local: true do |f| %>
      <%= f.label :category_id %>
      <%= f.text_field :category_id%>
      <%= f.label :title %>
      <%= f.text_field :title, value:@title%>
      <%= f.label :content %>
      <%= f.text_area :content %>
      <%= f.label :url%>
      <%= f.text_area :url, value:@url %>
      <%= f.label :published_at%>
      <%= f.text_field :published_at, value:@published_at %>
      <%= f.submit "記事の登録" %>
<% end %>
