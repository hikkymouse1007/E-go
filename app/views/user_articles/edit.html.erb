<%= form_with model:@article, url:user_article_path, local: true do |f| %>
      <%= f.label :category_id %>
      <%= f.text_field :category_id %>
      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.label :content %>
      <%= f.text_area :content %>
      <%= f.label :url %>
      <%= f.text_area :url %>
      <%= f.label :published_at %>
      <%= f.text_field :published_at %>
      <%= f.submit "記事の更新" %>
<% end %>
